#include <stdio.h>

int main()
{
	int a,b,c,n,i;
	char p;
	scanf("%d", &n);
	for(i=1;i<=n;i++)
	{
		scanf("%d", &a);
		p = getchar();
		if (p == '+')
		{
			scanf("%d", &b);
			c = a + b;
		}
		else if (p == '-')
		{
			scanf("%d", &b);
			c = a-b;
		}
		else if (p == '*')
		{
			scanf("%d", &b);
			c = a * b;
		}
		else if(p == '/')
		{
			scanf("%d", &b);
			c = a / b;
		}
		printf("%d\n",c);
	}
	return 0;
}
